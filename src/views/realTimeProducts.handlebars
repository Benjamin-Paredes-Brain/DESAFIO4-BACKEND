<div>
    <div>AÑADIR PRODUCTOS</div>
    <form id="addProductForm">
        <input type="text" name="title" placeholder="ingrese el nombre del producto a añadir">
        <button type="submit">AÑADIR</button>
    </form>


    <div>ELIMINAR PRODUCTOS</div>
    <form id="deleteProductForm">
        <input type="text" name="id" placeholder="ingrese el index del producto a eliminar">
        <button type="submit">ELIMINAR</button>
    </form>

    <div>LISTA DE PRODUCTOS</div>
    {{#each productData}}
    <div>{{this.title}}</div>
    {{/each}}
</div>
<script src="/socket.io/socket.io.js"></script>
<script src="/static/js/index.js"></script>